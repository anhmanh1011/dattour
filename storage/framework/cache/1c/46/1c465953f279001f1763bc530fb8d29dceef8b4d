<?php
    use Web\Hotel\Models\Hotel as Hotel;
    use Web\Hotel\Models\HotelType as Type;
    use Web\Hotel\Models\AmentitiesHotel as Amenities;
    use Web\Hotel\Models\Location as Location;
    use Web\Hotel\Models\States as States;

    function onStart(){ $this->prepareVars();}
    function onFilterHotel() { $this->prepareVars(); }
    function onPagination() { $this->prepareVars(); }
    function prepareVars() {
        $options = post('Filter', []);

        $this['hotel']      = Hotel::listFrontEnd($options);
        $this['states']     = States::get()->toArray();
        $this['amenities']  = Amenities::get()->toArray();
        $this['types']      = Type::get()->toArray();
        $this['pages']      = $this['hotel']->lastPage();
        $this['page']       = $this['hotel']->currentPage();
    }
    function onEnd(){
        $this->page->meta_title = "Hotel";
    }
?>
