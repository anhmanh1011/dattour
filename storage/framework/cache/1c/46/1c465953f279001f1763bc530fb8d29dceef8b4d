<script>
    // Ajax Tour
    (function ($) {

        $('#hotelFilter').on('change', 'input, select', function () {
            var $form = $(this).closest('form');
            $form.request();
            $('html, body').animate({
                scrollTop: $('.up_here').offset().top - 80
            }, 1000);
        });
        $('#Pagination').on('change', 'input, select', function () {
            $('html, body').animate({
                scrollTop: $('.up_here').offset().top - 80
            }, 1000);

            var $form = $(this).closest('form');
            $form.request();
        });
    })(jQuery);
</script>
